{
  "name": "news-analyzer",
  "homepage": "https://1ebobski.github.io/news-analyzer/main.html",
  "version": "0.1.0",
  "description": "news analyzer project",
  "main": "./src/index.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production rimraf dist && webpack --mode production",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "deploy": "cross-env NODE_ENV=production gh-pages -d dist",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.{js,ts,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,css,html}\"",
    "validate": "npm run lint && npm run format:check",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "keywords": [],
  "author": "1ebobski",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.26.0",
    "@types/core-js": "^2.5.8",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "autoprefixer": "^10.4.17",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prettier": "^5.2.3",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.8.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "core-js": "^3.36.0",
    "flickity": "^3.0.0"
  }
}
